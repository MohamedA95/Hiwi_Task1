{
    "name": "C100QVGGPre",
    "n_gpu": 1,

    "arch": {
        "type": "QuantVGG",
        "args": {
            "VGG_type":"D",
            "batch_norm": false,
            "num_classes": 1000,
            "pretrained_model": "/home/mmoursi/Hiwi_Task1/saved/models/Img_VGGD/2205_125025/model_best.pth"
        }
    },
    "data_loader": {
        "type": "ImageNet_data_loader",
        "args":{
            "data_dir": "/scratch/mmoursi/",
            "batch_size": 32,
            "shuffle": true,
            "validation_split": 0.1,
            "num_workers": 16,
            "pin_memory":true
        }
    },
    "test_data_loader": {
        "type": "ImageNet_data_loader",
        "args": {
            "data_dir": "/scratch/mmoursi/",
            "batch_size": 32,
            "download": false,
            "shuffle": true,
            "validation_split": 0.0,
            "num_workers": 4,
            "flavor": 100,
            "training": false
        }
    },

    "optimizer": {
        "type": "SGD",
        "args":{
            "lr": 0.01,
            "momentum": 0.9,
            "weight_decay": 0.0005
        }
    },
    "loss": "CrossEntropy",
    "metrics": [
        "accuracy", "top_k_acc"
    ],
    "lr_scheduler": {
        "type": "MultiStepLR",
        "args": {
            "milestones": [60, 120, 160],
            "gamma": 0.2
        }
    },
    "trainer": {
        "epochs": 20,
        "save_dir": "saved/",
        "save_period": 20,
        "verbosity": 1,
        "monitor": "max val_accuracy",
        "early_stop": -1,
        "tensorboard": false
    },
    "extract": false,
    "extractor":{
        "PE":1,
        "SIMD":1,
        "DATAWIDTH":64,
        "SEQUENCE_LENGTH":120000,
        "CLASS_LABEL_BITS":1,
        "MUL_BITS":16,
        "MUL_INT_BITS":8,
        "ACC_BITS":16,
        "ACC_INT_BITS":8,
        "IA_BITS":8,
        "IA_INT_BITS":4

    }
}
