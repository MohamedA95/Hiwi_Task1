{
    "name": "C100QVGG",
    "n_gpu": 1,

    "arch": {
        "type": "QuantVGG",
        "args": {
            "num_classes": 100,
            "VGG_type":"D",
            "batch_norm": false
        }
    },
    "data_loader": {
        "type": "CIFAR_data_loader",
        "args":{
            "data_dir": "/scratch/mmoursi/CIFAR_DATA/cifar-100",
            "batch_size": 512,
            "download": true,
            "shuffle": true,
            "validation_split": 0.0,
            "num_workers": 4,
            "flavor":100,
            "training":true
        }
    },
    "optimizer": {
        "type": "SGD",
        "args":{
            "lr": 0.01,
            "momentum": 0.9,
            "weight_decay": 0.0005
        }
    },
    "loss": "CrossEntropy",
    "metrics": [
        "accuracy", "top_k_acc"
    ],
    "lr_scheduler": {
        "type": "MultiStepLR",
        "args": {
            "milestones": [60, 120, 160],
            "gamma": 0.2
        }
    },
    "trainer": {
        "epochs": 200,
        "save_dir": "/home/mmoursi/Hiwi_Task1/saved/",
        "save_period": 200,
        "verbosity": 2,
        "monitor": "max accuracy",
        "early_stop": -1,
        "tensorboard": false
    },
    "extract": false,
    "extractor":{
        "PE":1,
        "SIMD":1,
        "DATAWIDTH":64,
        "SEQUENCE_LENGTH":120000,
        "CLASS_LABEL_BITS":1,
        "MUL_BITS":16,
        "MUL_INT_BITS":8,
        "ACC_BITS":16,
        "ACC_INT_BITS":8,
        "IA_BITS":8,
        "IA_INT_BITS":4
    }
}
